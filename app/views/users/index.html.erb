<h1>Users</h1>

<table>
	<tr><th><%= sortable "username" %></th>
		<th><%= sortable "rank" %></th>
		<th><%= sortable "division" %></th>
	</tr>
<% @user.each do |user| %>
	<tr>
	 <td><%= link_to user.kgs_names, user_path(user.kgs_names) %></td>
	 <td><%= rank_to_string(user.rank) %></td>
	 <td><%= user.division %></td>
	</tr>
<% end %>
</table>